<%- | String  $apihost,
      Integer $apiport,
      Hash    $repositories,
| -%>
#!/bin/bash
# 
# Managed by Puppet, do not edit manually
#
# Promote content between repositories and automatically publish
#
set -ex

<% $repositories.each | Hash $repository | { -%>
# Printing repository
<%= $repository %>
<% } -%>
